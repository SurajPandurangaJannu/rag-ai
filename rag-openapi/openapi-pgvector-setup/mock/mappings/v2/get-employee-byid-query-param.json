{
  "request": {
    "urlPath": "/v2/employee",
    "method": "GET",
    "queryParameters": {
      "employeeId": {
        "matches": "([a-zA-Z0-9]+)"
      }
    }
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "employeeId": "{{request.query.employeeId}}",
      "name": "Savitha",
      "gender": "female"
    },
    "headers": {
      "Content-Type": "application/json",
      "version": "v2"
    }
  }
}